{
    "manifest_version": 3,
    "name": "Language Substituter",
    "version": "0.1.0",
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["script.js"],
            "css": ["styles.css"]
        }
    ],
    "web_accessible_resources": [
        {
            "resources": ["freqList"],
            "matches": ["<all_urls>"]            
        }
    ]
}